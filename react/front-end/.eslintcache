Id\", 1);\r\n    localStorage.setItem(\"weekEnding\", summary.weekEnding);\r\n  };\r\n\r\n  componentDidMount() {\r\n    let userId = localStorage.getItem(\"userID\");\r\n    this.userId = 1;\r\n    axios\r\n      .get(`http://localhost:8084/timesheet/summary?userId=` + this.userId)\r\n      .then((res) => {\r\n        this.setState({ summarys: res.data });\r\n      });\r\n  }\r\n\r\n  handleShowMore = () => {\r\n    if (this.state.count === 5) {\r\n      this.setState({\r\n        count: this.state.summarys.length,\r\n        show: \"Show Less\",\r\n      });\r\n    } else {\r\n      this.setState({\r\n        count: 5,\r\n        show: \"Show More\",\r\n      });\r\n    }\r\n  };\r\n\r\n  render() {\r\n    const count = this.count;\r\n    return (\r\n      <div>\r\n        <table>\r\n          <tr>\r\n            <th>WeekEnding</th>\r\n            <th>Total Hours</th>\r\n            <th>Submission Status</th>\r\n            <th>Approval Status</th>\r\n            <th>Option</th>\r\n            <th>Comment</th>\r\n          </tr>\r\n          <tr></tr>\r\n          <tbody>{this.renderTableData()}</tbody>\r\n        </table>\r\n\r\n        <button\r\n          type=\"button\"\r\n          className=\"button-time\"\r\n          button-time\r\n          onClick={this.handleShowMore}\r\n        >\r\n          {this.state.show}\r\n        </button>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n",{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","severity":1,"message":"46","line":184,"column":9,"nodeType":"47","messageId":"48","endLine":184,"endColumn":15},{"ruleId":"45","severity":1,"message":"49","line":1,"column":17,"nodeType":"47","messageId":"48","endLine":1,"endColumn":25},{"ruleId":"45","severity":1,"message":"50","line":3,"column":10,"nodeType":"47","messageId":"48","endLine":3,"endColumn":18},{"ruleId":"45","severity":1,"message":"51","line":4,"column":10,"nodeType":"47","messageId":"48","endLine":4,"endColumn":20},{"ruleId":"45","sev